
# Maintainer: WinterMute <davem@devkitpro.org>
pkgname=armv4-pkg-config
pkgver=0.28
pkgrel=1
pkgdesc='pkg-config wrapper (for armv4 homebrew development)'
arch=('any')
url='http://devkitpro.org/'
source=('pkg-config.in')
sha256sums=('2cbb151a220c55490dd4a67c85e82321697c82b8e5ca6bcbed70d5ab467ff342')
groups=('gp2x-dev')

package() {
  install -d "$pkgdir"/opt/devkitpro/portlibs/armv4/bin
  cp "$srcdir"/pkg-config.in "$pkgdir"/opt/devkitpro/portlibs/armv4/bin/arm-none-eabi-pkg-config
}
